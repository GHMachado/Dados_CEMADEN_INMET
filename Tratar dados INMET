três primeiras colunas\n","\n","    # Renomear as colunas do df2\n","    df2.columns = ['Data Medicao', 'Hora Medicao', nome_estacao]\n","\n","    # Adicionar o DataFrame ao dicionário, usando o nome da estação como chave\n","    dataframes_dict[nome_estacao] = df2.set_index(['Data Medicao', 'Hora Medicao'])\n","\n","# Combinar todos os DataFrames em um único DataFrame, alinhando pelas datas e horas\n","df_final = pd.concat(dataframes_dict, axis=1)\n","\n","# Remover o nível superior do MultiIndex (nomes das estações)\n","df_final.columns = df_final.columns.droplevel(0)\n","\n","# Resetar o índice para transformar \"Data Medicao\" e \"Hora Medicao\" em colunas normais\n","df_final = df_final.reset_index()\n","\n","# Criar um MultiIndex para as colunas, onde a primeira linha são as datas e a segunda linha são as horas\n","colunas = pd.MultiIndex.from_arrays(\n","    [df_final['Data Medicao'], df_final['Hora Medicao']],\n","    names=['Data', 'Hora']\n",")\n","\n","# Criar um novo DataFrame com os dados das estações e o MultiIndex nas colunas\n","df_final_reorganizado = pd.DataFrame(df_final.drop(columns=['Data Medicao', 'Hora Medicao']).values.T, columns=colunas)\n","\n","# Adicionar os nomes das estações como índice do DataFrame\n","df_final_reorganizado.index = df_final.drop(columns=['Data Medicao', 'Hora Medicao']).columns\n","\n","# Remover a linha onde o nome da estação é \"nan\" (se existir)\n","df_final_reorganizado = df_final_reorganizado[df_final_reorganizado.index != 'nan']\n","\n","# Salvar o DataFrame final em um arquivo CSV\n","df_final_reorganizado.to_csv(os.path.join(pasta_csv, 'dados_combinados.csv'), sep=';')"],"metadata":{"id":"dHHeGWhyLSLv"},"execution_count":null,"outputs":[]}]}
